{
  "ConnectionStrings": {
    "SquirrelCoreDBConnection": "Server=localhost;Database=SquirrelCoreDB;Trusted_Connection=True;",
    "BlobStorageConnectionString": "UseDevelopmentStorage=true"
  },
  "MongoDatabase": {
    "ConnectionString": "",
    "DatabaseName": ""
  },
  "CoreWebAPIDomain": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UserDbChangesBlobContainerName": "user-db-changes",
  // Add path to python.exe  Used for registrating SqlFormatterService
  "PythonExePath": "C:\\Python310\\python.exe",
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": { "path": "Logs/webapi.log" }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/webapi-errors.log",
          "restrictedToMinimumLevel": "Warning"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Squirrel.SqlService.WebApi"
    }
  }
}
